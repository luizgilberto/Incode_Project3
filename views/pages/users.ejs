<%- include('header') -%>
<main>
    <h2>Here are the <%=users.length %> users in the database</h2>
    <% let num = 1 %> 
    <% users.forEach((user)=>{%>
      <h3>User:<%= num %></h3>
      <p><strong>Frist name:</strong>  <%= user.firstname %></p>
      <p><strong>Last name:</strong>  <%= user.lastname %></p>
      <p><strong>Email:</strong>  <%= user.email %></p>
      <p><strong>Password:</strong>  <%= user.password %></p>
      <% num ++ %> 
    <% }) %> 
  </main>
<%- include('footer') -%>